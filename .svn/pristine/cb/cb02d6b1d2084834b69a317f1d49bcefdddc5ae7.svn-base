package com.sinosoft.xf.dataPredict.domain;

import javax.persistence.Entity;
import javax.persistence.Table;
import com.sinosoftframework.core.domain.AudiEntity.AudiEntity;

/**
 * 信访件分布信息表实体类信息
 * @date 2017-08-12
 * @author liyang
 */
@Entity
@Table(name = "DATAPREDICT_DISTRIBUTION_INFO")
public class DataPredictDistributionInfo extends AudiEntity {
	
	private static final long serialVersionUID = 1L;
	
	private String issueRegionName;
	private int forecastNum;
	private int forecastDateFlag;
	private int issueRegionFlag;
	public String getIssueRegionName() {
		return issueRegionName;
	}
	public void setIssueRegionName(String issueRegionName) {
		this.issueRegionName = issueRegionName;
	}
	public int getForecastNum() {
		return forecastNum;
	}
	public void setForecastNum(int forecastNum) {
		this.forecastNum = forecastNum;
	}
	public int getForecastDateFlag() {
		return forecastDateFlag;
	}
	public void setForecastDateFlag(int forecastDateFlag) {
		this.forecastDateFlag = forecastDateFlag;
	}
	public int getIssueRegionFlag() {
		return issueRegionFlag;
	}
	public void setIssueRegionFlag(int issueRegionFlag) {
		this.issueRegionFlag = issueRegionFlag;
	}
	
	
}
