@charset "utf-8";
.bigautocomplete-layout{display:none;background-color:#d5e4fe;border:1px solid #BCBCBC;position:absolute;z-index:100;max-height:220px;overflow-x:hidden;overflow-y:auto; }
.bigautocomplete-layout table{border-collapse:collapse;border-spacing:0;background:none repeat scroll 0 0 #d5e4fe;width:100%;cursor:default;}
.bigautocomplete-layout table tr{background:none repeat scroll 0 0 #d5e4fe;}
.bigautocomplete-layout .optionsTitle{width:350px;height:21px;overflow:hidden;padding:1px 0px;float:left;text-overflow:ellipsis;white-space:nowrap}
.bigautocomplete-layout .optionsCount{width:100%;height:18px;padding:1px 0px;text-align:right;color: rgb(170, 170, 170);}
.bigautocomplete-layout .ct{background:none repeat scroll 0 0 #D2DEE8 !important;}
.bigautocomplete-layout div{word-wrap:break-word;word-break:break-all;padding:1px 5px;}